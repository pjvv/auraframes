# Aura Frames Configuration
# Copy this file to .env and fill in your values

# Required - Authentication
AURA_EMAIL=your-email@example.com
AURA_PASSWORD=your-password

# Optional - Debug mode (default: false)
AURA_DEBUG=false

# Optional - Device emulation
AURA_LOCALE=en-US
AURA_DEVICE_IDENTIFIER=0000000000000000
AURA_APP_IDENTIFIER=com.pushd.client

# Optional - Image proxy URL (default: https://imgproxy.pushd.com)
AURA_IMAGE_PROXY_URL=https://imgproxy.pushd.com

# Optional - AWS Cognito Identity Pools (required for upload functionality)
# AWS_UPLOAD_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
# AWS_SQS_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
